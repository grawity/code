#!/usr/bin/env python
import sys
import ipaddress

args = sys.argv[1:]

for arg in args:
    if "/" in arg:
        net = ipaddress.ip_network(arg)
        rev = net.reverse_pointer.split(".")
        if net.version == 6:
            full, bits = divmod(net.prefixlen, 4)
            full += 2
            if bits:
                full += 1
                start = int(rev[-full], 16)
                count = 2 ** (4 - bits)
                for i in range(count):
                    rev[-full] = "%x" % (start + i)
                    print(".".join(rev[-full:]))
            else:
                print(".".join(rev[-full:]))
        else:
            full, bits = divmod(net.prefixlen, 8)
            full += 2
            if bits:
                print("NOT IMPLEMENTED", rev)
            else:
                if net.prefixlen == 32:
                    rev[0] = "0"
                print(".".join(rev[-full:]))
    else:
        net = ipaddress.ip_address(arg)
        print(net.reverse_pointer)
