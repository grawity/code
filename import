#!/usr/bin/env bash
if [[ $1 == -[!-]* ]]; then
	MAGIC=1
fi
if [[ $MAGIC ]]; then
	exec /usr/bin/import "$@"
else
	cmd=$(cat /proc/$PPID/cmdline | tr \\0 \\n | head)
	if [[ $cmd = bash ]]; then
		echo "error: insufficient magic (did you forgot '#!/usr/bin/env' again?)"
		kill $PPID
		exit 42
	fi
fi
