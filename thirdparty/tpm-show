    TPM_BAD_TYPE = 52
    TPM_INVALID_RESOURCE = 53
    TPM_NOTFIPS = 54
    TPM_INVALID_FAMILY = 55
    TPM_NO_NV_PERMISSION = 56
    TPM_REQUIRES_SIGN = 57
    TPM_KEY_NOTSUPPORTED = 58
